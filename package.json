{
  "name": "nekostore",
  "version": "0.6.0",
  "description": "Real-time synced data-store like Firestore with type",
  "keywords": [
    "database",
    "firebase",
    "firestore",
    "real-time",
    "sync"
  ],
  "homepage": "https://github.com/esnya/nekostore#readme",
  "bugs": {
    "url": "https://github.com/esnya/nekostore/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esnya/nekostore.git"
  },
  "license": "MIT",
  "author": "esnya",
  "main": "lib/Nekostore.js",
  "scripts": {
    "build": "tsc -b",
    "ci-scripts": "run-s clean lint cover build docs examples",
    "clean": "rimraf docs/* lib",
    "cover": "nyc mocha",
    "dev": "run-p dev:*",
    "dev:build": "npm run -s build -- --watch",
    "dev:docs": "nodemon -i -w src -e ts -d 3 -x \"npm run -s docs\"",
    "dev:lint": "esw --fix --ext .ts src/**/*.ts examples/src/**/*.ts",
    "dev:test": "npm run -s test -- --watch",
    "docs": "typedoc --out docs --plugin typedoc-plugin-markdown --excludeNotExported --excludePrivate --exclude **/impl/**/*,**/*.spec.* --gitRevision master",
    "examples": "run-s examples:*",
    "examples:1_install": "cd examples && npm ci",
    "examples:2_build": "cd examples && npm run build",
    "examples:3_start": "node examples",
    "lint": "eslint src/**/*.ts examples/src/**/*.ts",
    "test": "cross-env TS_NODE_FILES=true mocha"
  },
  "dependencies": {
    "firebase": "^7.2.0",
    "lodash": "^4.17.15",
    "mingo": "^2.3.5",
    "path-to-regexp": "^3.1.0",
    "uuid": "^3.3.3",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@firebase/testing": "^0.14.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lodash": "^4.14.144",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.6",
    "@types/node-fetch": "^2.5.2",
    "@types/sinon-chai": "^3.2.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-watch": "^6.0.1",
    "firebase-tools": "^7.5.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.1",
    "mongodb": "^3.3.2",
    "node-fetch": "^3.1.1",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.10",
    "typescript": "^3.6.4",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "engines": {
    "node": ">=10"
  }
}
