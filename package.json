{
  "name": "nekostore",
  "version": "0.3.2",
  "description": "Realtime synced datastore like Firestore with type",
  "main": "lib/Nekostore.js",
  "scripts": {
    "build": "tsc -b",
    "ci-scripts": "run-s clean lint cover build docs examples",
    "clean": "rimraf docs/* lib",
    "cover": "nyc mocha",
    "docs": "typedoc --out docs --plugin typedoc-plugin-markdown --excludeNotExported --excludePrivate --exclude **/impl/**/*,**/*.spec.* --gitRevision master",
    "dev": "run-p dev:*",
    "dev:build": "npm run -s build -- --watch",
    "dev:docs": "nodemon -i -w src -e ts -x \"npm run -s docs\"",
    "dev:lint": "esw --fix --ext .ts **/*.ts --ignore-pattern **/*/lib/**/* --ignore-pattern **/node_modules/**/*",
    "dev:test": "npm run -s test -- --watch",
    "examples": "run-s examples:*",
    "examples:1_build": "cd examples && npm run build",
    "examples:2_start": "node examples",
    "lint": "eslint --ignore-pattern **/*/lib/**/* --ignore-pattern **/node_modules/**/* **/*.ts",
    "test": "cross-env TS_NODE_FILES=true mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esnya/nekostore.git"
  },
  "keywords": [
    "database",
    "realtime",
    "sync",
    "firebase",
    "firestore"
  ],
  "author": "esnya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esnya/nekostore/issues"
  },
  "homepage": "https://github.com/esnya/nekostore#readme",
  "devDependencies": {
    "@firebase/testing": "^0.13.1",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.1",
    "@types/node-fetch": "^2.5.0",
    "@types/sinon-chai": "^3.2.3",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.5.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-watch": "^6.0.0",
    "firebase-tools": "^7.3.2",
    "lodash": "^4.17.15",
    "mocha": "^6.2.0",
    "mongodb": "^3.3.2",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.2",
    "sinon-chai": "^3.3.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.2.5",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "firebase": "^6.6.1",
    "mingo": "^2.3.5",
    "path-to-regexp": "^3.1.0",
    "uuid": "^3.3.3"
  }
}
